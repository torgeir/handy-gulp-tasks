{
  "name": "handy-gulp-tasks",
  "version": "1.0.0",
  "description": "A set of handy, reusable gulp tasks.",
  "main": "server.js",
  "engines": {
    "node": "0.10.x"
  },
  "scripts": {
    "postinstall": "npm run compile",
    "start": "./node_modules/.bin/gulp server",
    "clean": "./node_modules/.bin/gulp clean",
    "lint": "./node_modules/.bin/gulp lint",
    "test": "./node_modules/.bin/gulp test",
    "watch": "GULP_IS_WATCH=true ./node_modules/.bin/gulp watch",
    "compile": "npm run clean && ./node_modules/.bin/gulp default",
    "compile-prod": "NODE_ENV=production npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/torgeir/handy-gulp-tasks.git"
  },
  "author": "@torgeir",
  "license": "mit",
  "bugs": {
    "url": "https://github.com/torgeir/handy-gulp-tasks/issues"
  },
  "homepage": "https://github.com/torgeir/handy-gulp-tasks",
  "dependencies": {
    "bluebird": "^2.6.2",
    "browserify": "^8.1.0",
    "chai": "^1.10.0",
    "compression": "^1.3.0",
    "connect": "^3.3.4",
    "del": "^1.1.1",
    "es6ify": "^1.6.0",
    "factor-bundle": "^2.3.3",
    "fastclick": "^1.0.3",
    "font-awesome": "^4.2.0",
    "gulp": "^3.8.10",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-cache": "^0.2.4",
    "gulp-compile-handlebars": "^0.4.4",
    "gulp-debug": "^2.0.0",
    "gulp-embedlr": "^0.5.2",
    "gulp-file-include": "^0.7.1",
    "gulp-htmlmin": "^0.2.0",
    "gulp-if": "^1.2.5",
    "gulp-imagemin": "^2.1.0",
    "gulp-jshint": "^1.9.0",
    "gulp-less": "^2.0.1",
    "gulp-livereload": "^3.2.0",
    "gulp-minify-css": "^0.3.11",
    "gulp-mocha": "^2.0.0",
    "gulp-notify": "^2.1.0",
    "gulp-replace": "^0.5.0",
    "gulp-rev": "^2.0.1",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-template": "^2.0.0",
    "gulp-uglify": "^1.0.2",
    "gulp-zip": "^2.0.2",
    "jshint-stylish": "^1.0.0",
    "lodash": "^2.4.1",
    "mkdirp": "^0.5.0",
    "open": "^0.0.5",
    "rimraf": "^2.2.8",
    "serve-static": "^1.8.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.2.1",
    "zepto-browserify": "^1.1.3-0"
  }
}
